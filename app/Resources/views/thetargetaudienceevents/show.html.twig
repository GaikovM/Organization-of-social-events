{% extends 'base.html.twig' %}

{% block body %}
    <h1>Thetargetaudienceevent</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ theTargetAudienceEvent.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ theTargetAudienceEvent.name }}</td>
            </tr>
            <tr>
                <th>Access</th>
                <td>{% if theTargetAudienceEvent.Access %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('thetargetaudienceevents_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('thetargetaudienceevents_edit', { 'id': theTargetAudienceEvent.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
